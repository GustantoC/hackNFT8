<%- include('./partial/header.ejs',{title: 'Home Page'}) %>
<%-include('./partial/navbar.ejs')%>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
        <th scope="col">Price</th>
        <th scope="col">Image</th>
        <th scope="col">Sell Availability</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% dataNFT.forEach(el => { %>
      <tr>
        <th scope="row"><%=el.id%></th>
        <td><%=el.name%></td>
        <td><%=el.description%></td>
        <td><%=el.price%></td>
        <td><img src="<%=el.imageUrl%>" style="max-height:200px;max-width:200px;height:auto;width:auto"></td>
        <td><%if(el.availability){%>Yes<% }else{ %>No<% } %></td>
        <td><a href="/nft/<%=el.id%>" class="btn btn-primary">See Detail</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<%- include('./partial/footer.ejs') %>
